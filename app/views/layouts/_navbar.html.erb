<header>
  <% case run_mode %>
  <% when :logged_out %>
    <%= render 'layouts/navbar_logout_mode' %>
  <% when :diary_mode %>
    <%= render 'layouts/navbar_diary_mode' %>
  <% when :memo_mode %>
    <%= render 'layouts/navbar_memo_mode' %>
  <% else %>
    <%= render 'layouts/navbar_logout_mode' %>
  <% end %>
</header>
